#include<iostream>
using namespace std;

int main(){
    int n,j=0; //declare adn inisitialize the variables n for size of array and j to keep track of the zeros
    cout<<"Enter the size of array:-";
    cin>>n;
    int arr[n]={0};  //assign all the elements of array to zero
    cout<<"Enter the elements of array:-"<<endl;
    for(int i=0;i<n;i++){
        
        cin>>arr[j];
        if(arr[j]!=0){
            j++;
        }
    }
    
    for(int i=0;i<n;i++){
        cout<<arr[i]<<" ";
    }
    
    
}
